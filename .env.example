# Application Settings
APP_NAME=ObaidaFramework
APP_ENV=development
APP_DEBUG=true
APP_URL=http://localhost/obaida
APP_KEY=

# Database Settings
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
